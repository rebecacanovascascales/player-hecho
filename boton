void main () {
animate (ejecutar);
}
void ejecutar() {

  control();
  imagen();
}
void imagen () {

  rect(120,120,80,80);
}
void control () {

  background (225,225,225);
  println("X: " + mouseX);
  println("Y: "+ mouseY);
    if (mousePressed)
      if(mouseButton==LEFTBUTTON){
      	if(mouseX > 120 && mouseX < 200&& mouseY> 120 && mouseY <200){
          
      background (255,0,0);
          ellipse (mouseX, mouseY,50,50);
        }}}
        
